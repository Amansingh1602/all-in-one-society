# Backend Environment Variables for Render
# Copy to .env and fill with your actual values

# MongoDB Connection (Replace with your MongoDB Atlas connection string)
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/society_m?retryWrites=true&w=majority

# JWT Secret (Generate a secure random string)
JWT_SECRET=your-super-secure-jwt-secret-key-here

# Port (Render will set this automatically)
PORT=5000

# Node Environment
NODE_ENV=production
